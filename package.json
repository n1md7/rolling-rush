{
  "name": "vite-template",
  "private": true,
  "description": "ViteJS + Typescript + Vitest",
  "author": {
    "name": "Harry Kodua",
    "email": "b.kodua@gmail.com",
    "url": "https://github.com/n1md7"
  },
  "license": "MIT",
  "main": "dist/main.js",
  "bugs": {
    "url": "https://github.com/n1md7/vite-JS/issues"
  },
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode=development",
    "build:github": "tsc && vite build --mode=github",
    "build:crazy": "tsc && vite build --mode=crazy",
    "build:itch": "tsc && vite build --mode=itch",
    "test": "vitest --mode=test --no-watch",
    "preview": "vite preview",
    "format": "npx lint-staged",
    "husky": "husky install"
  },
  "dependencies": {
    "@react-three/drei": "^9.83.9",
    "@react-three/fiber": "^8.14.1",
    "@react-three/rapier": "^1.1.1",
    "r3f-perf": "^7.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.156.1",
    "tiny-emitter": "^2.1.0",
    "tweakpane": "^4.0.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@tweakpane/core": "^2.0.1",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.46.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.3",
    "sass": "^1.66.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": "18.17.1",
    "npm": ">=9.0.0"
  },
  "engineStrict": true
}
